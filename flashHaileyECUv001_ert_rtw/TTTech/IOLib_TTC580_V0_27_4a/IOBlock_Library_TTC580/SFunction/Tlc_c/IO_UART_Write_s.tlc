%implements "IO_UART_Write_s" "C"

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%function BlockTypeSetup(block, system) void
%if (TargetType == "RT")
   		%assign ::IO_UART = "any"
        %<LibAddToCommonIncludes("IO_UART.h")>
%endif
%endfunction

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%function Start(block, system) Output
%if (TargetType == "RT")

	%assign len = LibBlockInputSignal(1, "", "",0)
    %assign len_type = LibBlockInputSignalDataTypeName(1,"")
    %% 1-based dimension info used for eventual subsequent dyn-ports
    %assign dimin1 = LibBlockInputSignalWidth(1)

	%assign data = LibBlockInputSignalAddr(0, "", "",0)
    %assign data_type = LibBlockInputSignalDataTypeName(0,"")

	%assign ErrorCode = LibBlockOutputSignal(0, "", "",0)  
    %assign ErrorCode_type = LibBlockOutputSignalDataTypeName(0,"")
    %% 1-based dimension info used for eventual subsequent dyn-ports
    %assign dimout0 = LibBlockOutputSignalWidth(0)
  	%if dimout0 > 1
	%assign yDyn_0_1 = LibBlockOutputSignal(0, "", "",1)  
    %<yDyn_0_1> = (%<ErrorCode_type>)%<rtw_param1>;  // additionally dimension of 1st port assigned to mask parameter
    %endif 
	%assign tx_len = LibBlockOutputSignalAddr(1, "", "",0)
    %assign tx_len_type = LibBlockOutputSignalDataTypeName(1,"")

	/* init_function call IO_UART_Write */



%endif
%endfunction

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%function Outputs(block, system) Output
%if (TargetType == "RT")

	%assign len = LibBlockInputSignal(1, "", "",0)
    %assign len_type = LibBlockInputSignalDataTypeName(1,"")
    %% 1-based dimension info used for eventual subsequent dyn-ports
    %assign dimin1 = LibBlockInputSignalWidth(1)

	%assign data = LibBlockInputSignalAddr(0, "", "",0)
    %assign data_type = LibBlockInputSignalDataTypeName(0,"")

	%assign ErrorCode = LibBlockOutputSignal(0, "", "",0)  
    %assign ErrorCode_type = LibBlockOutputSignalDataTypeName(0,"")
    %% 1-based dimension info used for eventual subsequent dyn-ports
    %assign dimout0 = LibBlockOutputSignalWidth(0)
  	%if dimout0 > 1
	%assign yDyn_0_1 = LibBlockOutputSignal(0, "", "",1)  
    %<yDyn_0_1> = (%<ErrorCode_type>)%<rtw_param1>;  // additionally dimension of 1st port assigned to mask parameter
    %endif 
	%assign tx_len = LibBlockOutputSignalAddr(1, "", "",0)
    %assign tx_len_type = LibBlockOutputSignalDataTypeName(1,"")

	/* function call of IO_UART_Write */

{


%<ErrorCode> = IO_UART_Write( %<data> , %<len> , %<tx_len> );

}
%endif
%endfunction

