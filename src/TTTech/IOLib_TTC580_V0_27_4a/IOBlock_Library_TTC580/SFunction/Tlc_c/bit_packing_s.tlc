%implements "bit_packing_s" "C"

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%function BlockTypeSetup(block, system) void
%if (TargetType == "RT")
   		%assign ::IOLib500_utils = "any"
        %<LibAddToCommonIncludes("IOLib500_utils.h")>
%endif
%endfunction

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%function Start(block, system) Output
%if (TargetType == "RT")
	%assign Endianness = rtw_param1  
	%assign Format = rtw_param2  
	%assign Inversion = rtw_param3  
	%assign Startbit = rtw_param4  
	%assign Width_bits = rtw_param5  
	%assign Factor = rtw_param6  
	%assign Offset = rtw_param7  

	%assign Data_in = LibBlockInputSignal(0, "", "",0)
    %assign Data_in_type = LibBlockInputSignalDataTypeName(0,"")
    %% 1-based dimension info used for eventual subsequent dyn-ports
    %assign dimin0 = LibBlockInputSignalWidth(0)


 
	%assign Data_out = LibBlockOutputSignalAddr(0, "", "",0)
    %assign Data_out_type = LibBlockOutputSignalDataTypeName(0,"")

	/* init_function call bit_packing */



%endif
%endfunction

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%function Outputs(block, system) Output
%if (TargetType == "RT")
	%assign Endianness = rtw_param1  
	%assign Format = rtw_param2  
	%assign Inversion = rtw_param3  
	%assign Startbit = rtw_param4  
	%assign Width_bits = rtw_param5  
	%assign Factor = rtw_param6  
	%assign Offset = rtw_param7  

	%assign Data_in = LibBlockInputSignal(0, "", "",0)
    %assign Data_in_type = LibBlockInputSignalDataTypeName(0,"")
    %% 1-based dimension info used for eventual subsequent dyn-ports
    %assign dimin0 = LibBlockInputSignalWidth(0)


 
	%assign Data_out = LibBlockOutputSignalAddr(0, "", "",0)
    %assign Data_out_type = LibBlockOutputSignalDataTypeName(0,"")

	/* function call of bit_packing */

{


bit_packing( %<Data_in> , %<Data_out> , %<Endianness> , %<Format> , %<Inversion> , %<Startbit> , %<Width_bits> , %<Factor> , %<Offset> );

}
%endif
%endfunction

